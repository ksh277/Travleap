{
  "name": "shinan-travel-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch server-api.ts\"",
    "dev:frontend": "vite",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "build:node": "node build.mjs",
    "build:vite": "npx vite build",
    "build:vercel": "node ./node_modules/vite/bin/vite.js build",
    "build:with-types": "npx tsc && npx vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite preview --port 3000",
    "typecheck": "npx tsc --noEmit",
    "ws": "node server/websocket-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run ws\"",
    "api": "tsx server-api.ts",
    "api:dev": "tsx watch server-api.ts",
    "setup-db": "tsx scripts/setup-database.ts"
  },
  "dependencies": {
